<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zaneco.readandbill"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.zaneco.readandbill.SplashScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zaneco.readandbill.MyReadingEntry"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.READINGENTRY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zaneco.readandbill.MyFieldFindingEntry"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.FIELDFINDINGENTRY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zaneco.readandbill.MyConsumerList"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.CONSUMERLIST" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zaneco.readandbill.SummaryList"            
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.SUMMARY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zaneco.readandbill.BatchPrintingSOA"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.BATCHPRINTINGSOA" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zaneco.readandbill.NewConsumerList"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.NEWCONSUMERLIST" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zaneco.readandbill.NewConsumerEntry"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.NEWCONSUMERENTRY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.zaneco.readandbill.BillhistoryList"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.BillhistoryList" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity android:name="com.androidapp.mytools.AppPreferenceActivity"></activity>
        
      
    </application>

</manifest>